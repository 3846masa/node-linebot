{
  "name": "@3846masa/linebot",
  "description": "LINE BOT API wrapper for Node.js",
  "version": "0.1.0",
  "author": "3846masa <3846masahiro+git@gmail.com>",
  "bugs": {
    "url": "https://github.com/3846masa/node-linebot/issues"
  },
  "dependencies": {
    "axios": "^0.14.0",
    "body-parser": "^1.15.2",
    "eventemitter2": "2.1.3",
    "express": "^4.14.0"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-eslint": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015-node4": "^2.1.0",
    "babel-preset-es2016": "6.16.0",
    "babel-preset-es2017": "6.16.0",
    "esdoc": "^0.4.8",
    "esdoc-importpath-plugin": "0.0.1",
    "eslint": "^3.7.1",
    "eslint-config-airbnb-base": "8.0.0",
    "eslint-plugin-import": "1.16.0",
    "fixpack": "^2.3.1",
    "mkdirp": "0.5.1",
    "npm-run-all": "^3.1.0",
    "rimraf": "2.5.4",
    "sane": "1.4.1",
    "typedoc": "0.4.5",
    "typescript": "1.8.10"
  },
  "files": [
    "README.md",
    "index.js",
    "package.json",
    "lib"
  ],
  "homepage": "https://github.com/3846masa/node-linebot#readme",
  "keywords": [
    "line",
    "linebot"
  ],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/3846masa/node-linebot.git"
  },
  "scripts": {
    "build": "npm-run-all clean build:init build:tsc build:babel build:esdoc",
    "build:babel": "babel es2015/ --out-dir lib/",
    "build:esdoc": "esdoc -c ./.esdoc.json",
    "build:init": "mkdirp lib es2015 doc",
    "build:tsc": "tsc --outDir ./es2015 --rootDir ./src -p ./",
    "build:typedoc": "typedoc --out doc/ src/",
    "clean": "rimraf lib es2015 doc",
    "fixpack": "fixpack",
    "test": "echo \"Error: no test specified\" && exit 1",
    "typings": "typings",
    "watch": "sane \"npm run build\" src --glob='**/*.ts' --wait=10"
  }
}
